{
  "private": true,
  "name": "homepage",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "build": "next build && node ./scripts/insert-mochinoa-aa-to-html.js",
    "start": "next start",
    "lint:ts": "eslint \"*/**/*.{js,ts,tsx}\" --cache",
    "lint:js": "eslint ./*.js --cache --ignore-pattern .eslintrc.js",
    "lint:json": "prettier \"*.json\"",
    "lint:style": "stylelint \"./**/*.tsx\" --syntax css-in-js",
    "format:ts": "yarn lint:ts --fix",
    "format:js": "yarn lint:js --fix",
    "format:json": "yarn lint:json --write",
    "format:style": "yarn lint:style --fix",
    "lint": "run-p -l -n -c --aggregate-output lint:*",
    "format": "run-p -l -n -c --aggregate-output format:*",
    "typecheck": "node ./scripts/typecheck.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "yarn typecheck"
    ]
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "clsx": "^1.1.1",
    "next": "11.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.10.2",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "next-offline": "^5.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stylelint": "^13.2.1",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^3.9.6"
  },
  "browserslist": {
    "production": [
      ">0.2% in JP",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
